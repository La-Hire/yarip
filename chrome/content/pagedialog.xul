                        </treecols>
                        <treechildren context="yarip-edit-menupopup"/>
                      </tree>
                      <hbox>
                        <button icon="add" label="&button.label.add;" oncommand="dialog.add()" accesskey="&button.accesskey.add;"/>
                        <button icon="remove" label="&button.label.remove;" oncommand="dialog.remove()"/>
                        <button icon="clear" label="&button.label.removeAll;" oncommand="dialog.removeAll()"/>
                        <spacer flex="1"/>
                      </hbox>
                    </vbox>
                  </tabpanel>
                  <tabpanel id="elementScriptList" flex="1">
                    <vbox flex="1">
                      <tree id="elementScriptList-tree" editable="true" persist="height" flex="1">
                        <treecols>
                          <treecol id="elementScriptList-tree-xpath" label="&treecol.label.xpath;" editable="true" persist="width" primary="true" flex="1"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementScriptList-tree-priority" label="&treecol.label.priority;" editable="true" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementScriptList-tree-reinject" label="&treecol.label.reinject;" type="checkbox" editable="true" hidden="true" persist="width hidden" tooltiptext="&treecol.tooltiptext.reinject;"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementScriptList-tree-created" label="&treecol.label.created;" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementScriptList-tree-lastFound" label="&treecol.label.lastFound;" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementScriptList-tree-found" label="&treecol.label.found;" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementScriptList-tree-notFound" label="&treecol.label.notFound;" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                        </treecols>
                        <treechildren context="yarip-edit-menupopup"/>
                      </tree>
                      <splitter/>
                      <textbox id="elementScriptList-textbox" multiline="true" oninput="dialog.setValue(this.value)" persist="height" flex="1"/>
                      <hbox>
                        <button icon="add" label="&button.label.add;" oncommand="dialog.add()" accesskey="&button.accesskey.add;"/>
                        <button icon="remove" label="&button.label.remove;" oncommand="dialog.remove()"/>
                        <button icon="clear" label="&button.label.removeAll;" oncommand="dialog.removeAll()"/>
                        <spacer flex="1"/>
                      </hbox>
                    </vbox>
                  </tabpanel>
                </tabpanels>
              </tabbox>
            </vbox>
          </tabpanel>
          <tabpanel id="content-tabpanel" flex="1">
            <vbox flex="1">
