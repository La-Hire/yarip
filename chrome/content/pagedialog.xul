                          <treecol id="contentWhitelist-tree-force" label="&treecol.label.force;" type="checkbox" editable="true" hidden="true" persist="width hidden" tooltiptext="&treecol.tooltiptext.forceWhitelist;"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
