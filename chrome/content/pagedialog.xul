<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>
<?xml-stylesheet href="chrome://yarip-os/skin/pagedialog.css" type="text/css"?>
<!DOCTYPE dialog SYSTEM "chrome://yarip/locale/pagedialog.dtd">
<dialog id="page-dialog"
  title="&dialog.title.managePages;"
  buttons="accept"
  buttonlabelaccept="&button.label.close;"
  onload="dialog.load()"
  ondialogaccept="dialog.unload()"
  ondialogcancel="dialog.unload()"
  persist="screenX screenY width height"
  width="800"
  height="600"
  xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <script type="application/x-javascript"><![CDATA[
    const Cu = Components.utils;
    var yarip = Cu.import("resource://yarip/yarip.jsm", null).wrappedJSObject;
    Cu.import("resource://yarip/map.jsm");
    Cu.import("resource://yarip/page.jsm");
    Cu.import("resource://yarip/list.jsm");
    Cu.import("resource://yarip/item.jsm");
  ]]></script>
  <script type="application/x-javascript" src="chrome://yarip/content/observer.js"/>
  <script type="application/x-javascript" src="chrome://yarip/content/pagedialog.js"/>
  <script type="application/x-javascript"><![CDATA[
    var dialog = new YaripPageDialog();
  ]]></script>

  <stringbundleset>
    <stringbundle id="page-dialog-stringbundle" src="chrome://yarip/locale/overlay.properties"/>
  </stringbundleset>

  <commandset>
    <command id="cmd_cut" oncommand="dialog.doCut();"/>
    <command id="cmd_copy" oncommand="dialog.doCopy();"/>
    <command id="cmd_paste" oncommand="dialog.doPaste();"/>
    <command id="cmd_goto" oncommand="dialog.gotoPage();"/>
<!--    <command id="cmd_selectall" oncommand="dialog.doSelectAll();"/>-->
    <command id="cmd_toggleExclusive" oncommand="dialog.toggleExclusive();"/>
  </commandset>

  <keyset>
    <key key="&key.key.cut;" modifiers="accel" command="cmd_cut"/>
    <key key="&key.key.copy;" modifiers="accel" command="cmd_copy"/>
    <key key="&key.key.paste;" modifiers="accel" command="cmd_paste"/>
    <key key="&key.key.goto;" modifiers="accel" command="cmd_goto"/>
<!--    <key key="&key.key.selectAll;" modifiers="accel" command="cmd_selectall"/>-->
  </keyset>

  <hbox flex="1">
    <tree id="tree-pages" width="200" editable="true" persist="width">
      <treecols>
        <treecol id="page" label="&listheader.label.pages;" editable="true" primary="true" flex="1"/>
      </treecols>
      <treechildren/>
    </tree>
    <splitter/>
    <vbox flex="1">
      <dialogheader id="dialogheader-page" description="&dialogheader.description.page;"/>
      <popupset>
        <menupopup id="yarip-edit-menupopup">
          <menuitem id="yarip-cut-menuitem" label="&menuitem.label.cut;" command="cmd_cut" class="menuitem-iconic"/>
          <menuitem id="yarip-copy-menuitem" label="&menuitem.label.copy;" command="cmd_copy" class="menuitem-iconic"/>
          <menuitem id="yarip-paste-menuitem" label="&menuitem.label.paste;" command="cmd_paste" class="menuitem-iconic"/>
          <menuseparator id="yarip-goto-menuseparator"/>
          <menuitem id="yarip-goto-menuitem" label="&menuitem.label.goto;" command="cmd_goto" class="menuitem-iconic"/>
        </menupopup>
      </popupset>
      <tabbox id="tabbox" flex="1">
        <tabs onselect="dialog.updateTab()">
          <tab id="element-tab" label="&tab.label.element;" oncommand="dialog.setUserSelect()"/>
          <tab id="content-tab" label="&tab.label.content;" oncommand="dialog.setUserSelect()"/>
          <tab id="page-tab" label="&tab.label.page;" oncommand="dialog.setUserSelect()"/>
        </tabs>
        <tabpanels flex="1">
          <tabpanel id="element-tabpanel" flex="1">
            <vbox flex="1">
              <tabbox id="element-tabbox" flex="1">
                <tabs onselect="dialog.updateTab()">
                  <tab id="elementWhitelist-tab" label="&tab.label.whitelist;" oncommand="dialog.setUserSelect()"/>
                  <tab id="elementBlacklist-tab" label="&tab.label.blacklist;" oncommand="dialog.setUserSelect()"/>
                  <tab id="elementAttributeList-tab" label="&tab.label.attribute;" oncommand="dialog.setUserSelect()"/>
                  <tab id="elementScriptList-tab" label="&tab.label.script;" oncommand="dialog.setUserSelect()"/>
                </tabs>
                <tabpanels flex="1">
                  <tabpanel id="elementWhitelist" flex="1">
                    <vbox flex="1">
                      <tree id="elementWhitelist-tree" editable="true" flex="1">
                        <treecols>
                          <treecol id="elementWhitelist-tree-xpath" label="&treecol.label.xpath;" editable="true" persist="width" primary="true" flex="1"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementWhitelist-tree-priority" label="&treecol.label.priority;" editable="true" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementWhitelist-tree-force" label="&treecol.label.force;" type="checkbox" editable="true" hidden="true" persist="width hidden" tooltiptext="&treecol.tooltiptext.forceWhitelist;"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementWhitelist-tree-created" label="&treecol.label.created;" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementWhitelist-tree-lastFound" label="&treecol.label.lastFound;" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementWhitelist-tree-found" label="&treecol.label.found;" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementWhitelist-tree-notFound" label="&treecol.label.notFound;" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementWhitelist-tree-ignored" label="&treecol.label.ignored;" hidden="true" persist="width hidden"/>
                        </treecols>
                        <treechildren context="yarip-edit-menupopup"/>
                      </tree>
                      <hbox align="center">
                        <button icon="add" label="&button.label.add;" oncommand="dialog.add()" accesskey="&button.accesskey.add;"/>
                        <button icon="remove" label="&button.label.remove;" oncommand="dialog.remove()"/>
                        <button icon="clear" label="&button.label.removeAll;" oncommand="dialog.removeAll()"/>
                        <spacer flex="1"/>
                        <checkbox id="elementWhitelist-exclusive" label="&label.value.exclusive;" checked="false" tooltiptext="&label.tooltiptext.elementExclusive;" command="cmd_toggleExclusive" accesskey="&button.accesskey.exclusive;"/>
                      </hbox>
                    </vbox>
                  </tabpanel>
                  <tabpanel id="elementBlacklist" flex="1">
                    <vbox flex="1">
                      <tree id="elementBlacklist-tree" editable="true" flex="1">
                        <treecols>
                          <treecol id="elementBlacklist-tree-xpath" label="&treecol.label.xpath;" editable="true" persist="width" primary="true" flex="1"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementBlacklist-tree-priority" label="&treecol.label.priority;" editable="true" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementBlacklist-tree-force" label="&treecol.label.force;" type="checkbox" editable="true" persist="width hidden" tooltiptext="&treecol.tooltiptext.forceBlacklist;"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementBlacklist-tree-placeholder" label="&treecol.label.placeholder;" type="checkbox" editable="true" hidden="true" persist="width hidden" tooltiptext="&treecol.tooltiptext.placeholder;"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementBlacklist-tree-created" label="&treecol.label.created;" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementBlacklist-tree-lastFound" label="&treecol.label.lastFound;" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementBlacklist-tree-found" label="&treecol.label.found;" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementBlacklist-tree-notFound" label="&treecol.label.notFound;" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementBlacklist-tree-ignored" label="&treecol.label.ignored;" hidden="true" persist="width hidden"/>
                        </treecols>
                        <treechildren context="yarip-edit-menupopup"/>
                      </tree>
                      <hbox>
                        <button icon="add" label="&button.label.add;" oncommand="dialog.add()" accesskey="&button.accesskey.add;"/>
                        <button icon="remove" label="&button.label.remove;" oncommand="dialog.remove()"/>
                        <button icon="clear" label="&button.label.removeAll;" oncommand="dialog.removeAll()"/>
                        <spacer flex="1"/>
                      </hbox>
                    </vbox>
                  </tabpanel>
                  <tabpanel id="elementAttributeList" flex="1">
                    <vbox flex="1">
                      <tree id="elementAttributeList-tree" editable="true" flex="1">
                        <treecols>
                          <treecol id="elementAttributeList-tree-xpath" label="&treecol.label.xpath;" editable="true" persist="width" primary="true" flex="1"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
