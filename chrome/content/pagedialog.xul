                  <tab id="contentHeader-tab" label="&tab.label.header;" oncommand="dialog.setUserSelect()"/>
                  <tab id="contentRedirectList-tab" label="&tab.label.redirect;" oncommand="dialog.setUserSelect()"/>
<!--                  <tab id="contentStreamList-tab" label="&tab.label.stream;" oncommand="dialog.setUserSelect()"/>-->
