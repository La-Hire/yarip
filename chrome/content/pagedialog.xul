                          <splitter class="tree-splitter" resizeafter="farthest"/>
                          <treecol id="elementAttributeList-tree-priority" label="&treecol.label.priority;" editable="true" hidden="true" persist="width hidden"/>
                          <splitter class="tree-splitter" resizeafter="farthest"/>
